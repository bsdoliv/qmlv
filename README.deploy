depends
    libqt4-declarative:amd64                  
    libqt4-network:amd64                      
    libqt4-script:amd64                       
    libqt4-scripttools:amd64                  
    libqt4-sql:amd64                          
    libqt4-sql-mysql:amd64                    
    libqt4-sql-sqlite:amd64                   
    libqt4-svg:amd64                          
    libqt4-xml:amd64                          
    libqt4-xmlpatterns:amd64                  
