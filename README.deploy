deploy instructions
    % tar -C /opt -zxvf ./casui.tgz
    % sudo /opt/mysql56/bin/mysqld.sh start
    % /opt/mysql56/bin/mysql -uroot --protocol tcp < /opt/casui/lib/deploy_cas_db_dump.sql

starting
    % sudo /opt/mysql56/bin/mysqld.sh start
    % /opt/casui/bin/casui
